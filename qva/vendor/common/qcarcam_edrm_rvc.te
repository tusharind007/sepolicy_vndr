type vendor_qcarcam_edrm_rvc, domain;
type vendor_qcarcam_edrm_rvc_exec, exec_type, vendor_file_type, file_type;

init_daemon_domain(vendor_qcarcam_edrm_rvc)

allow vendor_qcarcam_edrm_rvc vendor_camera_socket:dir search;
allow vendor_qcarcam_edrm_rvc vendor_camera_socket:sock_file write;
allow vendor_qcarcam_edrm_rvc hal_graphics_allocator_default:fd use;
allow vendor_qcarcam_edrm_rvc vendor_ais_server:unix_stream_socket connectto;
allow vendor_qcarcam_edrm_rvc ion_device:chr_file rw_file_perms;
allow vendor_qcarcam_edrm_rvc gpu_device:chr_file { ioctl map open read write };
allow vendor_qcarcam_edrm_rvc graphics_device:chr_file { ioctl open read write };
allow vendor_qcarcam_edrm_rvc hal_graphics_mapper_hwservice:hwservice_manager find;
allow vendor_qcarcam_edrm_rvc hwservicemanager:binder call;
allow vendor_qcarcam_edrm_rvc hwservicemanager_prop:file { getattr map open read };
allow vendor_qcarcam_edrm_rvc vendor_sysfs_kgsl:file { getattr open read };
allow vendor_qcarcam_edrm_rvc vendor_early_services:dir r_dir_perms;
allow vendor_qcarcam_edrm_rvc vendor_early_services:file r_file_perms;
allow vendor_qcarcam_edrm_rvc vendor_sysfs_kpi:file w_file_perms;
